{% style %}
  .pg-affiliate {
    header {
      padding-top: 65%;
      h1 {
        margin-bottom: 8px;
        font-size: 36px;
      }
      p {
        color: #fff;
        font-family: $headline_font;
        font-size: 16px;
        text-transform: uppercase;
        @media (min-width: 768px) {
          font-size: 24px;
        }
      }
      ul {
        list-style-type: none;
        padding: 0;
        margin: 0;
        li {
          a.button {
            display: block;
            width: 100%;
            max-width: 420px;
            margin: 6px auto;
            padding: 0 20px;
          }
        }
        @media (min-width: 768px) {
          display: flex;
          flex-flow: row nowrap;
          justify-content: space-around;
          width: 100%;
          max-width: 480px;
          margin: 0 auto;
          padding: 0 18px;
          li {
            width: 45%;
            flex-basis: 45%;
            a {
              max-width: 100%;
              margin: 0 auto;
            }
          }
        }
      }
      @media (min-width: $tablet-large) {
        padding-top: 37%;
        h1 {
          margin-bottom: 16px;
          font-size: 67px;
        }
      }
    }
  }

  .affboxes {
    .contain {
      h3 {
        text-align: center;
        font-size: 22px;
        line-height: 38px;
        color: #353935;
        padding: 0 18px;
        width: 100%;
        margin: 20px auto 30px;
        max-width: 900px;
        font-family: $body_font;
        font-weight: 400;
      }
    }
    .content-inner {
      min-height: 437px;
    }
    .box-content {
      background: #fff;
    }
    .grid-item:hover {
      .grid-header {
        transform: none;
      }
    }
    .grid {
      .grid-item {
        .v-top {
          position: static !important;
        }
        .v-bottom {
          position: static !important;
          .grid-footer {
            transform: none;
            position: static;
            a {
              color: $pale-olive-green;
              text-decoration: underline;
              em {
                display: block;
              }
              &:hover {
                color: #353535;
              }
            }
          }
        }
      }
      .grid-header {
        color: #a3a3a3;
        h4,
        strong {
          color: #000;
          opacity: 1;
        }
      }
    }
    .twitfeed {
      display: none;
      @media (min-width: $tablet-large) {
        display: block;
      }
    }
    .payout {
      .content-inner {
        min-height: 431px;
      }
      .v-top {
        background: #e4e4e4;
        .grid-header {
          padding: 0;
        }
        h6 {
          text-transform: uppercase;
          font-family: $open-sans;
          color: #a3a3a3;
          padding: 7px 12px;
          display: inline-block;
          background: #fff;
          @media (min-width: $tablet-large) {
            padding: 12px 24px;
          }
        }
        h4 {
          background: #fff;
          border-bottom: 1px solid #e4e4e4;
          padding: 15px 12px;
          margin-bottom: 0;
          @media (min-width: $tablet-large) {
            padding: 15px 24px;
          }
        }
      }

      ul {
        padding: 0;
        margin: 0;
        list-style-type: none;
        li {
          background: #fff;
          border-bottom: 1px solid #e4e4e4;
          display: flex;
          flex-flow: row nowrap;
          justify-content: space-between;
          padding: 14px 12px;
          @media (min-width: $tablet-large) {
            padding: 23px 24px;
          }
          span {
            font-size: 14px;
          }
          span:first-child {
            font-family: $open-sans;
            color: #111;
            font-weight: 600;
          }
          span:last-child {
            color: #a3a3a3;
          }
          &.last {
            background: $pale-olive-green;
            color: #fff;
            font-family: $open-sans;
            text-align: center;
            text-transform: uppercase;
            font-size: 20px;
            @media (min-width: $tablet-large) {
              padding: 28px 24px;
            }
          }
        }
      }
    }
  }

